{
  "name": "fdc3-tester",
  "version": "1.0.0",
  "description": "This utility allows testing of FDC3 broadcasting.",
  "scripts": {
    "start": "concurrently npm:start:web npm:start:openfin",
    "start:web": "serve -l 5501",
    "start:openfin": "openfin --launch --config http://localhost:5501/app.json"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "openfin-cli": "^3.0.2",
    "serve": "^11.3.2"
  }
}
